{
    "name": "NoFecebook",
    "version": "1.0",
    "description": "Disable Facebook news feed",
    "content_scripts": [
        {
            "js": [
                "main.js"
            ],
            "run_at": "document_end",
            "matches": [
                "https://www.facebook.com/*"
            ]
        }
    ],
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "manifest_version": 2
}